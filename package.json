{
  "name": "lite-bulletproof-react",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@hookform/resolvers": "^3.3.2",
    "@tailwindcss/typography": "^0.5.10",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "dompurify": "^3.0.6",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime": "^4.0.1",
    "nanoid": "^5.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-helmet-async": "^2.0.3",
    "react-hook-form": "^7.49.2",
    "react-router-dom": "^6.21.0",
    "type-fest": "^4.8.3",
    "typescript": "^5.3.3",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@hookform/devtools": "^4.3.1",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/dompurify": "^3.0.5",
    "@types/faker": "^6.6.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "faker": "^6.6.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "sass": "^1.69.5",
    "serve": "^14.2.1",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.10",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-commonjs": "^0.10.1",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imp": "^2.4.0",
    "vite-plugin-static-copy": "^1.0.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-plugin-utils": "^0.4.3",
    "vite-tsconfig-paths": "^4.2.2"
  },
  "config": {
    "prv": {
      "dev": "vite preview --mode=dev --port=4022 --open=http://localhost:4022/",
      "demo": "vite preview --mode=demo --port=4033 --open=http://localhost:4033/"
    },
    "stop": "for /f \"tokens=3 delims=LISTENING\" %i in ('netstat -ano ^| findStr \"4002\" ^| findStr \"LISTENING\"') do @Taskkill /F /IM %i"
  },
  "scripts": {
    "start-dev": "vite --mode=dev --host",
    "start-demo": "vite --mode=demo --host",
    "build-dev": "tsc && vite build --mode=dev",
    "build-demo": "tsc && vite build --mode=demo",
    "build-stgng": "tsc && vite build --mode=staging",
    "build-prod": "tsc && vite build --mode=prod",
    "prv-dev": "%npm_package_config_prv_dev%",
    "prv-demo": "%npm_package_config_prv_demo%",
    "stop": "%npm_package_config_stop%",
    "stop-all": "taskkill -F -IM node.exe",
    "release-all": "net stop winnat",
    "inst": "npm i --legacy-peer-deps"
  }
}
